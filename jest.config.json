{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "app/*.{js,jsx}",
    "app/**/*.{js,jsx}",
    "app/**/**/*.{js,jsx}",
    "app/**/**/**/*.{js,jsx}"
  ],
  "unmockedModulePathPatterns": [
    "node_modules/react/",
    "node_modules/enzyme/",
    "node_modules/jest/"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__mocks__/fileMock.js",
    "\\.(css|scss)$": "<rootDir>/app/__mocks__/styleMock.js"
  },
  "coverageDirectory": "./coverage"
}
